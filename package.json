{
  "name": "React-Boilerplate",
  "version": "0.1.0",
  "description": "React Boilerplate",
  "main": "server.js",
  "scripts": {
    "lint": "eslint ./; true",
    "prod": "better-npm-run prod",
    "start": "npm-run-all --parallel express pack",
    "express": "better-npm-run express",
    "build": "better-npm-run build",
    "pack": "better-npm-run webpack"
  },
  "betterScripts": {
    "prod": {
      "command": "node server",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "node webpack/build.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "express": {
      "command": "node server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "webpack": {
      "command": "babel-node webpack/webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://meidy88@bitbucket.org/verysmartdonkey/donkey-web-analytics.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/verysmartdonkey/donkey-web-analytics#readme",
  "dependencies": {
    "browser-request": "^0.3.3",
    "classnames": "^2.2.4",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "history": "^4.6.2",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-tooltip": "^3.3.0",
    "redux": "^3.5.1",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-form": "^6.6.3",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.0.1",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "0.0.15",
    "clean-webpack-plugin": "^0.1.16",
    "cli-color": "^1.2.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.10.2",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-watch": "^3.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "force-case-sensitivity-webpack-plugin": "^0.2.1",
    "less-loader": "^4.0.3",
    "lru-memoize": "^1.0.1",
