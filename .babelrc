{
  "presets": ["react","es2015","stage-0"],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  },
  "plugins": [
    ["module-resolver", {
        "root": ["./"],
        "alias":{
          "actions":"./client/actions",
          "constants":"./client/constants",
          "containers":"./client/components/commons/containers",
          "commons":"./client/components/commons",
          "layouts":"./client/components/commons/layouts",
          "images":"./assets/images",
          "pages":"./client/components/pages",
          "coreLib":"./client/coreLib",
          "utils":"./client/utils",
          "utils-coreLib":"./client/coreLib/utils",
          "styles":"./client/styles",
          "Icon":"./client/containers/Common/Icon/Icon"
        }
      }],
    "transform-decorators-legacy"

  ],
}
